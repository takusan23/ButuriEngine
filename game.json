{
	"width": 640,
	"height": 320,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"karaoke": {
			"type": "image",
			"width": 80,
			"height": 72,
			"path": "image/karaoke.png"
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"toomo": {
			"type": "image",
			"width": 75,
			"height": 75,
			"path": "image/toomo.png"
		},
		"pentagon": {
			"type": "image",
			"width": 100,
			"height": 95,
			"path": "image/pentagon.png"
		},
		"inu": {
			"type": "image",
			"width": 100,
			"height": 83,
			"path": "image/inu.png"
		},
		"old_toomo": {
			"type": "image",
			"width": 100,
			"height": 81,
			"path": "image/old_toomo.png"
		}
	},
	"environment": {
		"sandbox-runtime": "2",
		"niconico": {
			"supportedModes": [
				"ranking"
			]
		}
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-box2d/lib/BodyType.js",
		"node_modules/@akashic-extension/akashic-box2d/lib/Box2D.js",
		"node_modules/@akashic-extension/akashic-box2d/lib/Box2DOptions.js",
		"node_modules/@akashic-extension/akashic-box2d/lib/index.js",
		"node_modules/box2dweb/box2d.js"
	],
	"moduleMainScripts": {
		"@akashic-extension/akashic-box2d": "node_modules/@akashic-extension/akashic-box2d/lib/index.js",
		"box2dweb": "node_modules/box2dweb/box2d.js"
	}
}